{
    "name": "character-count",
    "displayName": "Character Count",
    "description": "Markdown Character Count - a status bar contribution that reports out the number of characters in a Markdown document as you interact with it.",
    "version": "0.0.1",
    "publisher": "stevensona",
    "license": "MIT",
    "author": {
        "name": "stevensona"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}